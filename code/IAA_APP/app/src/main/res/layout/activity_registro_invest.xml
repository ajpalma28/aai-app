<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityRegistroInvest">

    <EditText
        android:id="@+id/editInvestDNI"
        android:layout_width="308dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="60dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Introduzca su contraseña"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="414dp" />

    <EditText
        android:id="@+id/editInvestContr"
        android:layout_width="308dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="440dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Introduzca sus Apellidos"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="129dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Introduzca su Fecha de Nacimiento"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="320dp" />

    <EditText
        android:id="@+id/editInvestName"
        android:layout_width="308dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="252dp" />

    <EditText
        android:id="@+id/editInvestNac"
        android:layout_width="308dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="346dp" />

    <EditText
        android:id="@+id/editInvestConfContr"
        android:layout_width="308dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="534dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:text="Introduzca su DNI/NIF"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="34dp" />

    <EditText
        android:id="@+id/editInvestApell"
        android:layout_width="308dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="155dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirme su contraseña"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="508dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Introduzca su Nombre"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="226dp" />

    <Button
        android:id="@+id/btnAceptReg"
        android:layout_width="129dp"
        android:layout_height="49dp"
        android:text="ACEPTAR"
        android:onClick="registerInvest"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="649dp" />

    <Button
        android:id="@+id/btnCancReg"
        android:layout_width="129dp"
        android:layout_height="49dp"
        android:text="Cancelar"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="649dp" />

    <CheckBox
        android:id="@+id/checkTerminos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="He leído y acepto los términos y condiciones"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="592dp" />

</androidx.constraintlayout.widget.ConstraintLayout>